<h1>Listing urls</h1>

<table>
  <thead>
    <tr>
      <th>Alias</th>
      <th>Http</th>
      <th>Uuid</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @urls.each do |url| %>
      <tr>
        <td><%= url.alias %></td>
        <td><%= url.http %></td>
        <td><%= url.UUID %></td>
        <td><%= url.user_id %></td>
        <td><%= link_to 'Show', url %></td>
        <td><%= link_to 'Edit', edit_url_path(url) %></td>
        <td><%= link_to 'Destroy', url, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Url', new_url_path %>
